{
  "name": "nestjs-docker",
  "version": "1.0.0",
  "description": "",
  "license": "MIT",
  "author": "",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/main.js",
    "start:ts": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "debug": "nodemon -L --config nodemon-docker-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "build:prod": "rimraf dist && npm run build",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.10",
    "@nestjs/core": "^11.0.10",
    "@nestjs/microservices": "^11.0.10",
    "@nestjs/platform-express": "^11.0.10",
    "@nestjs/typeorm": "^11.0.0",
    "@nestjs/websockets": "^11.0.10",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cronbee": "^0.6.1",
    "dotenv": "^8.2.0",
    "nest-commander": "^3.16.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.1.0",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@nestjs/testing": "^11.0.10",
    "@types/express": "4.17.6",
    "@types/jest": "25.2.3",
    "@types/node": "14.0.5",
    "@types/supertest": "2.0.9",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "prettier": "2.0.5",
    "supertest": "4.0.2",
    "ts-jest": "^29.2.5",
    "ts-node": "10.9.2",
    "tsconfig-paths": "3.9.0",
    "tslint": "5.15.0",
    "typescript": "4.7"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
